{
    "files.exclude": {
	  "**/.git": true,
	  "**/.svn": true,
	  "**/.hg": true,
	  "**/CVS": true,
	  "**/.DS_Store": true,
	  "**/*.meta":true,
	  "**/*.gitignore":true,
	  "**/*.csproj":true,
	  "**/*.sln":true,
	  "**/*.unity":true,
	  "**/*.mat":true,
	  "**/*.ttf":true,
	  "**/*.wav":true,
	  "**/*.mp3":true,
	  "**/*.rar":true,
	  "**/*.zip":true,

	  "**/*.vs":true,
	  "**/*.vscode":true,
	  "**/ProjectSettings":true,
	  "**/Packages":true,
	  "**/Temp":true,
	  "**/obj":true,
	  "**/Logs":true,
	  "**/Library":true,
	},
	"editor.autoClosingQuotes": "beforeWhitespace",
	"editor.codeLens": false,
	"editor.fontSize": 13.2,
	"editor.trimAutoWhitespace": false,
	"workbench.sideBar.location": "right",
	"editor.minimap.enabled": false,
	"window.zoomLevel": 0,
	"editor.insertSpaces": false
	
}